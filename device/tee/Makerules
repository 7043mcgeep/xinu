# This Makefile contains rules to build files in the device/tee/ directory.

# Name of this component (the directory this file is stored in)
COMP = device/tee

# Source files for this component
C_FILES = teeAlloc.c     \
          teeClose.c     \
          teeControl.c   \
          teeGetc.c      \
          teeOpen.c      \
          teePutc.c      \
          teeRead.c      \
          teeWrite.c
S_FILES =

# Add the files to the compile source path
DIR = ${TOPDIR}/${COMP}
COMP_SRC += ${S_FILES:%=${DIR}/%} ${C_FILES:%=${DIR}/%}
